<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>admin Product</title>
    <link rel="stylesheet" href="/css/header.css">
    <link rel="stylesheet" href="/css/index.css">

</head>
<body>
    <header class="header">
        <nav class="nav">
            <ul class="nav-container">
                <li class="nav-links"><a class="active"  href="/">shop</a></li>
                <li class="nav-links"><a class="active"  href="/products">products</a></li>
                <li class="nav-links"><a class="active"  href="/cart">cart</a></li>
                <li class="nav-links"><a class="active"  href="/orders">orders</a></li>
                <li class="nav-links"><a class="active"  href="/add-product">Add Product</a></li>
                <li class="nav-links"><a class="active"  href="/admin-product">Admin Products</a></li>
            </ul>
        </nav>
    </header>
    <section class="content-wrapper">
        <% if(products.length > 0){ %>
            <% for(let product of products){ %>
                <article class="content">
                    <h2><%= product.title%></h2>
                    <img src="<%= product.imageUrl%>" alt="first book">
                    <h2>$<%= product.price%></h2>
                    <p><%= product.description%></p>
                    <div class="edit">
                        <button class="editbtn"><a href="/add-product/<%= product.id%>?edit=true"> Edit </a></button>
                        <form action="/delete" method="POST">
                            <input type="hidden" name="id" value="<%= product.id %>" >
                            <button type="submit">Delete</button>
                        </form>
                    </div>
                </article>
            <%}%>
        <%}else{%>
            <h1>yet to add product</h1>
        <%}%>
        <!-- <article class="content">
            <h2>first  book</h2>
            <img src="data:image/jpeg;base64," alt="first book">
            <h2>$13</h2>
            <p>the book of ice and fire</p>
            <form action="#" method="post">
                <button type="submit">Add to Cart</button>

            </form>
        </article> -->
    </section>
    <script src="../public/js/index.js">

    </script>
</body>
</html>